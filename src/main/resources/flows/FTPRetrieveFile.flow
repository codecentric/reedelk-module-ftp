{
  "id": "50c3bf91-d3da-4b94-b9a6-1edff964b387",
  "title": "FTP Download File",
  "description": "FTP Download File Flow",
  "flow": [
    {
      "implementor": "com.reedelk.rest.component.RESTListener",
      "description": "REST Listener",
      "configuration": {"ref": "858cc32a-fc85-4b87-acf1-e03f492e9416"},
      "path": "/ftp",
      "method": "GET",
      "streaming": "AUTO",
      "response": {
        "body": "#[message.payload()]",
        "status": 200
      },
      "errorResponse": {
        "body": "#[error]",
        "status": 500
      }
    },
    {
      "implementor": "com.reedelk.core.component.VariableSet",
      "description": "Variable Set",
      "name": "fileName",
      "mimeType": "text/plain",
      "value": "#[message.attributes().queryParams.fileName[0]]"
    },
    {
      "implementor": "com.reedelk.core.component.LoggerComponent",
      "description": "Logger",
      "level": "INFO",
      "message": "#[context.fileName]"
    },
    {
      "implementor": "com.reedelk.ftp.component.FTPRetrieve",
      "description": "FTP Retrieve",
      "connection": {"ref": "4a47934f-309a-4173-a420-26dca9f3a5b7"},
      "path": "#[context.fileName]",
      "autoMimeType": true
    }
  ]
}