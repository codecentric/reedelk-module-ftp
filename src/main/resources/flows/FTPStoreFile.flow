{
  "id": "29f52658-10c7-40ef-a8a7-e941d4278d37",
  "title": "FTP Upload File",
  "description": "FTP Upload File Flow",
  "flow": [
    {
      "implementor": "com.reedelk.rest.component.RESTListener",
      "description": "REST Listener",
      "configuration": {"ref": "858cc32a-fc85-4b87-acf1-e03f492e9416"},
      "path": "/ftp",
      "method": "POST",
      "streaming": "AUTO",
      "response": {
        "body": "#[message.payload()]",
        "status": 200
      },
      "errorResponse": {
        "body": "#[error]",
        "status": 500
      }
    },
    {
      "implementor": "com.reedelk.core.component.VariableSet",
      "description": "Variable Set",
      "name": "uploadedFileName",
      "mimeType": "text/plain",
      "value": "#[message.payload.myFile.attributes.filename]"
    },
    {
      "implementor": "com.reedelk.ftp.component.FTPStore",
      "description": "FTP Store",
      "connection": {"ref": "4a47934f-309a-4173-a420-26dca9f3a5b7"},
      "path": "#[context.uploadedFileName]",
      "content": "#[message.payload.myFile.payload]"
    }
  ]
}