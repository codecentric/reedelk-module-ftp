{
  "id": "e2fea75b-2b93-4b09-9816-60ccf2d41720",
  "title": "GET Hello world flow",
  "description": "Returns 'Hello world' text from GET /api/message",
  "flow": [
    {
      "implementor": "com.reedelk.rest.component.RESTListener",
      "description": "GET Hello World REST Listener",
      "configuration": {"ref": "858cc32a-fc85-4b87-acf1-e03f492e9416"},
      "path": "/message/{name:.*}",
      "method": "GET",
      "streaming": "AUTO",
      "response": {
        "body": "#[message.payload()]",
        "status": 200
      },
      "errorResponse": {
        "body": "#[error]",
        "status": 500
      }
    },
    {
      "implementor": "com.reedelk.core.component.PayloadSet",
      "description": "Payload Set",
      "mimeType": "*/*",
      "payload": "#[{ one: 'one123', two: 'two234' }]"
    },
    {
      "implementor": "com.reedelk.core.component.ScriptEvaluator",
      "description": "Script",
      "mimeType": "text/plain",
      "script": "test.js"
    },
    {
      "implementor": "com.reedelk.core.component.LoggerComponent",
      "description": "Logger",
      "level": "DEBUG",
      "message": "#['yep']"
    },
    {
      "implementor": "com.reedelk.ftp.component.DownloadFile",
      "description": "FTP Download File"
    },
    {
      "implementor": "com.reedelk.ftp.component.ListFiles",
      "description": "FTP List Files"
    }
  ]
}