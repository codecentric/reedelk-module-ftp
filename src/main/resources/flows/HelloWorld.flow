{
  "id": "e2fea75b-2b93-4b09-9816-60ccf2d41720",
  "title": "GET Hello world flow",
  "description": "Returns 'Hello world' text from GET /api/message",
  "flow": [
    {
      "implementor": "com.reedelk.rest.component.RESTListener",
      "description": "GET Hello World REST Listener",
      "configuration": {"ref": "858cc32a-fc85-4b87-acf1-e03f492e9416"},
      "path": "/message/{name:.*}",
      "method": "GET",
      "streaming": "AUTO",
      "response": {
        "body": "#[message.payload()]",
        "status": 200,
        "headers": {
          "rrrff32": "#['dddd']",
          "assd432kkl": "#['sup23er322222zz34']"
        }
      },
      "errorResponse": {
        "body": "#[error]",
        "status": 500,
        "headers": {"fffff": "#[]"}
      }
    },
    {
      "implementor": "com.reedelk.core.component.PayloadSet",
      "description": "Payload Set",
      "mimeType": "text/plain",
      "payload": "#[{ one: 'one123', two: 'two234432234' }]"
    },
    {
      "implementor": "com.reedelk.ftp.component.FTPStore",
      "description": "FTP Store"
    },
    {
      "implementor": "com.reedelk.ftp.component.FTPDelete",
      "description": "FTP Delete"
    }
  ]
}